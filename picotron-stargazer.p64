local world_meta = {}

function get_world_meta()
	return world_meta
end

function load_world(wname)
	mkdir(SAVEGAME_DIRECTORY)
	mkdir(SAVEGAME_DIRECTORY .. "/" .. wname)
	local meta_path = SAVEGAME_DIRECTORY .. "/" .. wname .. "/meta.pod"
	local tp, size, path = fstat(meta_path)
	if tp then
		local loaded_meta = fetch(meta_path)
		world_meta.seed = loaded_meta.seed
		dbg("Read world!")
	else
		world_meta.seed = stat(86)
		local a = store(meta_path, world_meta)
		dbg("Stored world!")
		dbg(a)
	end
end
