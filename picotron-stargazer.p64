--[[pod_format="raw",created="2025-06-30 15:08:35",modified="2025-10-14 18:02:22",revision=396]]

include "chunk.lua"
include "util.lua"
include "logging.lua"

function make_ticking_lib(chunk) -- module

	local lib = {}
	local get_chunk_by_world_pos = nil
	local get_tile_in_world = nil
	local set_tile_in_world = nil

	local chunk = nil
	
	function lib.meta(wants)
		chunk = wants "chunk"
		get_chunk_by_world_pos = chunk.get_chunk_by_world_pos
		get_tile_in_world = chunk.get_tile_in_world
		set_tile_in_world = chunk.set_tile_in_world
	end
	
	function enqueue(x,y,fx,fy)
		local c = get_chunk_by_world_pos(x,y)
		if c then 
			c.next_schedule[x .. " " .. y] = {x,y}
		else
			if fx and fy then 
				local fc = get_chunk_by_world_pos(fx,fy)
				fc.next_schedule[fx .. " " .. fy] = {fx,fy}
			end
		end
	end

	local function make_fluid_rule(
		this_tile, 
		residue_tile,
		stale_tile,
		left_tile,
		right_tile,
		free_tile,
		direction_bias
	)
		return function(x,y)
			local found_tile = get_tile_in_world(x,y+1)
			if found_tile == free_tile or (found_tile and fget(256+found_tile,7)) then
				-- downwards motion
				set_tile_in_world(x,y,0,false,true)
				set_tile_in_world(x,y+1,10,false,true)
				enqueue(x,y-1,x,y)
				enqueue(x,y+1,x,y)
				enqueue(x+1,y,x,y)
				enqueue(x-1,y,x,y)
			else
				-- lateral motion
				local delta_x = sgn(rnd()-0.5+direction_bias)
				local next_tile = delta_x < 0 and left_tile or right_tile
				-- first try
				local found_tile = get_tile_in_world(x+delta_x,y)
				if found_tile == free_tile or (found_tile and fget(256+found_tile,7)) then
					-- move
					set_tile_in_world(x,y,residue_tile,false,true)
					set_tile_in_world(x+delta_x,y,next_tile,false,true)
					-- enqueue free tiles and my next position
					enqueue(x,y-1,x,y)
					enqueue(x,y+1,x,y)
					enqueue(x+1,y,x,y)
					enqueue(x-1,y,x,y)
				else
					delta_x *= -1
					next_tile = delta_x < 0 and left_tile or right_tile
					-- second try
					local found_tile = get_tile_in_world(x+delta_x,y)
					if found_tile == free_tile or (found_tile and fget(256+found_tile,7)) then
						-- move
						set_tile_in_world(x,y,residue_tile,false,true)
						set_tile_in_world(x+delta_x,y,next_tile,false,true)
						-- enqueue free tiles and my next position
						enqueue(x,y-1,x,y)
						enqueue(x,y+1,x,y)
						enqueue(x+1,y,x,y)
						enqueue(x-1,y,x,y)
					else
						-- become still
						set_tile_in_world(x,y,stale_tile,false,true)
					end
				end
			end
		end
	end
	
	RULES = {
		[10] = make_fluid_rule(
			10, -- this_tile, 
			 0, -- residue_tile,
			10, -- stale_tile,
			34, -- left_tile,
			35, -- right_tile
			 0, -- free_tile,
			 0  -- direction_bias
		),
		[34] = make_fluid_rule(
			34, -- this_tile, 
			 0, -- residue_tile,
			10, -- stale_tile,
			34, -- left_tile,
			35, -- right_tile
			 0, -- free_tile,
		  -0.45 -- direction_bias
		),
		[35] = make_fluid_rule(
			35, -- this_tile, 
			 0, -- residue_tile,
			10, -- stale_tile,
			34, -- left_tile,
			35, -- right_tile
			 0, -- free_tile,
		   0.45 -- direction_bias
		),
	}
	
	local RESUME_COUNT = 0
	tick_schedule = cocreate(function() 
		while true do
			RESUME_COUNT += 1
			for _,c in pairs(chunk.loaded_chunks) do
				if c.y%3 != RESUME_COUNT%3 then goto continue end
				local next_action
				local action_count = 0
				repeat
					next_action = deli(c.schedule,1)
					if next_action then
						local x, y = unpack(next_action)
						local rule = get_tile_in_world(x,y)
						local r = RULES[rule]
						if (r) r(x,y)			
					end
				until not next_action
				::continue::
			end
			yield()
			for _,c in pairs(chunk.loaded_chunks) do
				for _,v in pairs(c.next_schedule) do
					c.schedule[#c.schedule+1] = v
				end
				c.next_schedule = {}
			end
			yield()
		end
	end)
	
	
	return lib

end -- of module
PUBLISH("ticking", make_ticking_lib())
