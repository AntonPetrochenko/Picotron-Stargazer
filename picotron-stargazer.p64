--[[pod_format="raw",created="2025-07-10 16:56:02",modified="2025-07-14 16:18:09",revision=11]]
include "constants.lua"

function make_decoder(sock)
	local wanted_length = 0,
	local whole_packet_accumulator = ''
	local stream_accumulator = ''

	local handlers = {}	

	function encode_int_be(num, len)
	    local bytes = {}
	    for i = len, 1, -1 do
	        local shift = (i - 1) * 8
	        local byte = (num >> shift) & 0xFF
	        bytes[#bytes+1] = byte
	    end
	    return chr(unpack(bytes))
	end
	
	function decode_int_be(str, len)
	    len = len or #str
	    local num = 0
	    for i = 1, len do
	        local byte = ord(str, i) or 0
	        num = (num << 8) | byte
	    end
	    return num
	end
	
	function consume(n)
		local s = decoder.stream_accumulator
		local retval = sub(s,1,n)
		decoder.stream_accumulator = sub(s,n+1)
		return retval
	end
	
	function decoder.read()
		local buf = sock:read()
		stream_accumulator = stream_accumulator .. buf
		
		-- Decoder's job is to accumulate enough bytes to have a whole packet
		-- Decoder will keep consuming bytes until it gets a complete packet
		
		while #stream_accumulator > 0 do
			if wanted_length == 0 then
				-- We don't have anything to consume. Next payload MUST be a packet length
				-- Consume 4 bytes from stream_acc
				local len_data = consume(n)
				wanted_length = len_data
			else
				local partial_data = consume(wanted_length)
				wanted_length = wanted_length - #partial_data
				whole_packet_accumulator = whole_packet_accumulator .. partial_data
				if wanted_length == 0 then
					-- Welp, we have a packet!
					local evt = unpod(whole_packet_accumulator)
					whole_packet_accumulator = ''
					handlers[evt.type](evt.payload)
				end
			end
		end
	end
	
	function decoder.on(evt, cb)
		handlers[evt] = cb
	end
	
	return decoder
end
