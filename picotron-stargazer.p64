local injectables = {}
local di_booted = false
	
function PUBLISH(id, obj)
	injectables[id] = obj
	obj.__injectable_id = id
end

local function wants(id)
	if not di_booted then error "Premature import!" end
	if injectables[id] then return injectables[id] else error( id .. " is not published!", 2) end
end

function DI_GET(id)
	return wants(id)
end
	
function DI_BOOT()
	di_booted = true
	for id, injectable in pairs(injectables) do
		if (not injectable.meta) then error (id .. " is malformed!") end
		injectable.meta(wants)
	end
end

