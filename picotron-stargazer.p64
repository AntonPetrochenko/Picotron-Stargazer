		for i,v in pairs(elements) do
		
			-- this is possible, apparently
			mouse_state.g_mouse_x, 
			mouse_state.g_mouse_y, 
			mouse_state.g_mouse_b, 
			mouse_state.g_wheel_x, 
			mouse_state.g_wheel_y 
				= mouse()
			
			mouse_state.left_down      =   mouse_state.g_mouse_b & 0x1 > 0
			mouse_state.right_down     =   mouse_state.g_mouse_b & 0x2 > 0
			mouse_state.wheel_delta    = -(mouse_state.g_wheel_y == 0 and 0 or sgn(mouse_state.g_wheel_y))
			
