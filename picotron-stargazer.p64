			if (b.x == "next") x_accumulator += (prev.width or error("No width! Can't calculate next position") ) + 2
			b.x = x_accumulator
		else
			if (b.x == "reset") then
				b.x = x_anchor
				x_accumulator = x_anchor
			else
				x_anchor = b.x
				x_accumulator = b.x
			end
		end
		if (b.init) b:init()
		prev = b
	end

	function ui.text(text,x,y,c,__skip)
		obj = {
			text = text,
			x = x,
			y = y,
			c = c,
			update = _nop,
			draw = function(e)
				print(e.text,e.x,e.y,e.c)
			end
		}
		-- width = 4px padding + length * 5
		obj.width = 4 + #text*5
		-- height = constant 10 = letter height * 5 // TODO count newlines
		obj.height = 10
		if (not __skip) put(obj)
		return obj
	end
	
	function ui.button(text,x,y,c_bg,c_text,c_border,cb,__skip)
		local obj = ui.text(text,x,y,c_text,true)
		obj.c_bg     = c_bg or 7
		obj.c_text   = c_text or 1
		obj.c_border = c_border or 1
		obj.c_text   = c_text
		-- origin = 2px 2px
		obj.draw = function (e)
			rectfill(e.x, e.y, e.x+e.width, e.y+e.height, e.c_bg)
			rect(e.x-1, e.y-1, e.x+e.width, e.y+e.height, e.c_border)
			print(e.text,e.x+2,e.y+2, e.c_text)
		end
		
		make_clickable(obj, cb or function(e)
			e.c_bg = 10
		end)
		
		if (not __skip) put(obj)
		return obj
	end
	
	function ui.spr(idx,x,y,__skip)
		obj = {
			idx = idx,
			x = x,
			y = y,
			update = _nop,
			draw = function(e)
				spr(idx,x,y)
			end
		}
		put(obj)
		
		if (not __skip) put(obj)
		return obj
	end
	
	function ui.field(x,y,width,ph,__skip)
		local obj = ui.button('',x,y)
		obj.input = ''
		obj.width = width or 64
		obj.c_bg = 0
		obj.c_border = 7
		
		function obj.update(e)
			if text_focus == e then
				e.c_border = 10
			else
				e.c_border = 7
			end
			e.text = #e.input > 0 and e.input or ph
		end
		make_clickable(obj, function (e)
			text_focus = e
		end)
		
		

		if (not __skip) put(obj)
		return obj
	end
	
	function ui.rect(x1,y1,x2,y2,c,__skip)
		local obj = {
			x1 = x1,
			y1 = y1,
			x2 = x2,
			y2 = y2,
			update = _nop,
			draw = function(e)
				rect(e.x1,e.y1,e.x2,e.y2,e.c)
			end
		}
		if (not __skip) put(obj)
		return obj
	end
	
	function ui.clear()
		elements = {}
	end
	
	function ui.pad(x,y)
		x_accumulator += x
		y_accumulator += y
	end
	

	return ui
end

