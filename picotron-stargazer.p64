	local y_accumulator = 0
	local x_accumulator = 0
	local x_anchor = 0
	
	local mouse_state = {}
	local PREV_MOUSE_LEFT = false
	local PREV_MOUSE_RIGHT = false
	
	function ui.update()
		if text_focus then
			text_focus.input = text_focus.input or ""
			if peektext() then
			  text_focus.input ..= readtext()
			end
			if keyp("backspace") then
				text_focus.input = sub(text_focus.input,1,#text_focus.input-1)
			end
