function make_game(chunk, ticking_lib, queue_lib, e_lib)
	
	local game = {}
	
	local entity_lib = nil
	function game.add_entity_lib(lib)
		entity_lib = lib
	end
	
	local queue_lib = nil
	local work_queues, update_world_mouse, draw_world_mouse
	local ticking_lib = nil
	local chunk = nil
	
	function game.meta(wants)
		queue_lib = wants "queue"
		work_queues = queue_lib.work_queues
		
		ticking_lib = wants "ticking"
		
		chunk = wants "chunk"
		entity_lib = wants "entity"
		
		local wi = wants "world_interaction"
		update_world_mouse = wi.update_world_mouse
		draw_world_mouse = wi.draw_world_mouse
	end

	local CAMERA_OFFSET_X = 0
	local CAMERA_OFFSET_Y = 0
	local CHUNK_X = 0
	local CHUNK_Y = 0
	local BBS_WARN = false	
	local FRAME_COUNTER = 0
	
	window { cursor = 'crosshair', pauseable = false, autoclose = false }
	
	function game.init(opts)
		poke(0x550b, 0x3f)
		memcpy(0x5000+63*4,0x5000+12*4,4)

		CHUNK_PROCESS = nil 

		if not (#opts.connection.host > 0) then
			if env().sandbox then
				chunk.set_fallback_behavior() BBS_WARN = true 
			else
				CHUNK_PROCESS = create_process("chunkservice.lua", {argv={pid()}} )	
			end 
		else 
			game.setup_multiplayer_consumer(opts.connection.host)
		end
		init_everything()
		chunk.init(opts.world)
		on_event("load_chunk", chunk.recv_chunk)
		entity_lib.add_player(build_player(chunk))
		chunk.set_center(CAMERA_OFFSET_X, CAMERA_OFFSET_Y)
		chunk_loader = chunk.start_loader_routine()
		chunk_saver  = chunk.start_saver_routine()
	end
	
	function game.update()
		if not (key("tab") and key("p")) then
			SCHEDULE_STATUS, SCHEDULE_OUTPUT = coresume(tick_schedule)
			if SCHEDULE_OUTPUT then error(SCHEDULE_OUTPUT) end
			LOADER_STATUS, LOADER_OUTPUT = coresume(chunk_loader)
			SAVER_STATUS = coresume(chunk_saver)
		end
	
		if	key("q") then
			save_all()
		end
		
		update_world_mouse(CAMERA_OFFSET_X, CAMERA_OFFSET_Y)
		if FRAME_COUNTER > 150 then
			entity_lib.update_entities(CAMERA_OFFSET_Y)
		end
		
		CAMERA_OFFSET_X = entity_lib.p.x-240
		CAMERA_OFFSET_Y = entity_lib.p.y-135
		chunk.set_center(CAMERA_OFFSET_X, CAMERA_OFFSET_Y)
	end
	
	function game.draw()

		cls(63)
		start_bar_frame()
		FRAME_COUNTER+=1
		if CAMERA_OFFSET_Y < 140 then
			spr(255,-(CAMERA_OFFSET_X/2)%480,-CAMERA_OFFSET_Y/2-40)
			spr(255,-(CAMERA_OFFSET_X/2)%480-480,-CAMERA_OFFSET_Y/2-40)
		end
		camera(CAMERA_OFFSET_X,CAMERA_OFFSET_Y)
		chunk.render_loaded_chunks()
		entity_lib.draw_entities(CAMERA_OFFSET_X, CAMERA_OFFSET_Y)
		chunk.render_light()
		draw_world_mouse()
	
		camera()
	
		sspr(256+ACTIVE_BLOCK, 0, 0, 8, 8, 400, 32, 16, 16)
		local mem = stat(3)
		if (key('tab')) then 
			mem = stat(0)
			drawlog(1, 30)
		end
		prints("\n CS: " .. (SAVER_STATUS and "OK" or "FAIL") ..
			"   CL: " .. (LOADER_STATUS and "OK" or "FAIL") ..
			"   TS: " .. (SCHEDULE_STATUS and "OK" or "FAIL") ..
			"   CC: " .. real_count(chunk.loaded_chunks),0,0,7)
		perf_graph()
	
		-- This is done in draw because Picotron skips draw on lag frames
		local a,b = queue_lib.work_queues()
		
		print(CAMERA_OFFSET_Y,0,0,7)
		

	end

	function game.setup_multiplayer_consumer(address)
		error ("Consumer set up on " .. address)
	end
	return game
end

